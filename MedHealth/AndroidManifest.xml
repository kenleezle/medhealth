<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Hackathon.medhealth"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.Hackathon.medhealth.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.Hackathon.medhealth.New_Medicine"
            android:parentActivityName=".MainActivity" >
        </activity>
        
        <activity
            android:name="com.Hackathon.medhealth.New_Timing"
            android:parentActivityName=".MainActivity" >
        </activity>
        
         <activity
            android:name="com.Hackathon.medhealth.MedicineTakingScreen"
            android:exported="true"
            android:parentActivityName=".MainActivity" >
        </activity>
        
        <service android:name="Service_Medicine_time" >
        </service>
        <activity android:name="Contacts"></activity>
        <activity android:name="contactinfo"></activity>
    </application>

</manifest>
